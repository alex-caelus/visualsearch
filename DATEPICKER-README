USAGE:

callbacks : {
    // These are the facets that will be autocompleted in an empty input.
    facetMatches : function(callback) {
        return callback([
            {
                label: 'To',   
                category: 'date', 
                DatepickerOptions: {},
            },
            {
                label: 'From',   
                category: 'dateWithButton', 
                DatepickerOptions: {
                    showOn: 'both',
                    buttonImage : 'http://www.floridatheatre.com/DesktopModules/EventsCalendar/images/CalendarViewIcon.jpg',
                    buttonImageOnly : true,
                    buttonText : 'choose a date'
                }
            },
        ]);
    },